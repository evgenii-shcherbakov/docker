{
  "name": "docker",
  "version": "1.0.0",
  "scripts": {
    "mongodb": "docker-compose -f docker-compose/docker-compose.mongodb.yml -p mongodb up",
    "mongodb:rs": "docker-compose -f docker-compose/docker-compose.mongodb-rs.yml -p mongodb-rs up",
    "postgresql": "docker-compose -f docker-compose/docker-compose.postgresql.yml -p postgresql up",
    "nats": "docker-compose -f docker-compose/docker-compose.nats.yml -p nats up",
    "redis": "docker-compose -f docker-compose/docker-compose.redis.yml -p redis up",
    "kafka": "docker-compose -f docker-compose/docker-compose.kafka.yml -p kafka up"
  }
}
